<%= simple_form_for @parent_product do |ppod| %>
  
<div class="col-xs-12 col-md-6 col-md-offset-3">
	<%= ppod.input :sku %>
	<%= ppod.input :usual_production_price %>
	<%= ppod.input :usual_production_currency %>
	<%= ppod.input :photo %>
 	<%= ppod.input :photo_cache, as: :hidden %>
	<%= ppod.button :submit, class: "cogs_btn" %>
<% end %>
</div>