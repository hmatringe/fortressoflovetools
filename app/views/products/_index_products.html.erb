<table class="table table-striped">
    <tr>
      <th>SKU</th>
      <!-- <th>parentSKU</th> -->
      <th>name</th>
      <!-- <th>size</th>
      <th>color</th>
      <th>structure</th>
      <th>category</th> -->
      <th></th>
    </tr>
    <% @products.each do |product| %>
      <tr>
        <td><%= link_to product.SKU, product_path(product) %></td>
        <!-- <td><%# product.parentSKU %></td> -->
        <td><%= product.name %></td>
        <!-- <td><%# product.size %></td>
        <td><%# product.color %></td>
        <td><%# product.structure %></td>
        <td><%# product.category %></td> -->
        <td><%= link_to product, method: :delete, class: "destroy"  do %>
          <i class="fa fa-trash" data-toggle="tooltip" title="Delete product. This action can not be undone!" aria-hidden="true"></i>
        <% end %>
        </td>
        <!-- <td><%# link_to "Delete", product, method: :delete, class: "destroy"  %></td> -->
      </tr>
    <% end %>
  </table>
